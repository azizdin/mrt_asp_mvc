@model IEnumerable<MRTmvc.Models.Route>




@{
    ViewBag.Title = "ViewHistory";
}
<div class="container-fluid">
    <div style="overflow:auto; height:700px;">
        <table class="table table-striped table-bordered" id="test1">
            <thead class="thead-dark">
                <tr>
                    <th scope="col" colspan="9" class=" text-center"> View History </th>
                </tr>
            </thead>
            <tbody>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Email)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.tripType)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.LevelCategory)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.StationFrom)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.StationTo)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.NumTicket)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DateTime)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Charge)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.DiscountPercent)
                    </th>
                </tr>

                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.tripType)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.LevelCategory)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.StationFrom)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.StationTo)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.NumTicket)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DateTime)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Charge)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.DiscountPercent)
                        </td>
                    </tr>

                }
            </tbody>
        </table>
    </div>
</div>