@model MRTmvc.Models.Route

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";
}



<div class="container">
    <h2>Details</h2>
    <table class="table table-borderless">
        <tbody>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.UserID)</th>
                <td>@Html.DisplayFor(model => model.UserID)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.Email)</th>
                <td>@Html.DisplayFor(model => model.Email)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.tripType)</th>
                <td>@Html.DisplayFor(model => model.tripType)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.LevelCategory)</th>
                <td>@Html.DisplayFor(model => model.LevelCategory)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.StationFrom)</th>
                <td>@Html.DisplayFor(model => model.StationFrom)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.StationTo)</th>
                <td>@Html.DisplayFor(model => model.StationTo)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.NumTicket)</th>
                <td>@Html.DisplayFor(model => model.NumTicket)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.DateTime)</th>
                <td>@Html.DisplayFor(model => model.DateTime)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.Charge)</th>
                <td>@Html.DisplayFor(model => model.Charge)</td>
            </tr>
            <tr>
                <th scope="row">@Html.DisplayNameFor(model => model.DiscountPercent)</th>
                <td>@Html.DisplayFor(model => model.DiscountPercent)</td>
            </tr>
        </tbody>
    </table>
    @Html.ActionLink("Back", "UserRoute", null, new { @class = "btn btn-primary", style = "margin:10px;" })
</div>